n    }\r\n\r\n    return(\r\n        <>\r\n            <ul className=\"pagination\">\r\n                {\r\n                    entries.map((item, i) => {\r\n                        if (entries.length < 5) return;\r\n                        if ((i % 5) === 0) {\r\n                            let location: any = window.location.pathname.match(/\\d/ig);\r\n                            let page: number = Number(location?.join(''));\r\n                            let pathName: string = window.location.pathname.replace(/\\d/ig, '');\r\n\r\n                            return(\r\n                                <li className=\"pagination__item\" key={i}>\r\n                                    <Link className={`pagination__link ${page === i ? 'pagination__item_active' : ''}`} \r\n                                        to={`${pathName}${i}`} \r\n                                        onClick={handlePagination}>\r\n\r\n                                    </Link>\r\n                                </li>\r\n                            )\r\n                        }\r\n                    })\r\n                }\r\n            </ul>\r\n        </>\r\n    )\r\n}\r\n\r\nexport default Pagination;","D:\\Web_design\\Node_JS\\Ideas\\Todo_GIT\\client\\src\\Info\\Info.tsx",[],"D:\\Web_design\\Node_JS\\Ideas\\Todo_GIT\\client\\src\\Utils\\handlePathname.ts",[],"D:\\Web_design\\Node_JS\\Ideas\\Todo_GIT\\client\\src\\Entries\\EntriesItem\\EntriesItem.tsx",[],"D:\\Web_design\\Node_JS\\Ideas\\Todo_GIT\\client\\src\\Button\\Button.tsx",[],{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","severity":1,"message":"52","line":64,"column":6,"nodeType":"53","endLine":64,"endColumn":8,"suggestions":"54"},{"ruleId":"55","severity":1,"message":"56","line":194,"column":57,"nodeType":"57","messageId":"58","endLine":194,"endColumn":59},{"ruleId":"55","severity":1,"message":"59","line":37,"column":36,"nodeType":"57","messageId":"58","endLine":37,"endColumn":38},{"ruleId